{
    "plugins": ["mocha", "chai"],
    "env": {
        "es2021": true,
        "node": true,
        "commonjs": true,
        "mocha": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-underscore-dangle": "off",
        "no-console": "off",
        "linebreak-style": "off",
        "import/no-extraneous-dependencies": ["error", {"devDependencies": true}]
    }
}
